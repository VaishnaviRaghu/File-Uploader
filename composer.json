{
    "name": "wkuploader/file-upload",
    "type": "library",
    "description": "File Upload functionality.",
    "keywords": [
        "wkuploader",
        "file-upload"
    ],
    "homepage": "https://github.com/VaishnaviRaghu/WK-Uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Vaishnavi R",
            "email": "vaishnavi.r@impelsys.com"
        }
    ],
    "require": {
        "php" : "~7.2",
        "flowjs/flow-php-server": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "WkUploader\\FileUpload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WkUploader\\FileUpload\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
